{
  "name": "@atomist/atomist-internal-sdm",
  "version": "0.3.1",
  "description": "an Atomist SDM to deliver Clojure projects",
  "author": "Atomist, Inc.",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/atomist/clojure-sdm.git"
  },
  "keywords": [
    "atomist",
    "automation",
    "sdm",
    "clojure",
    "lein"
  ],
  "homepage": "https://github.com/atomist/clojure-sdm#readme",
  "bugs": {
    "url": "https://github.com/atomist/clojure-sdm/issues"
  },
  "dependencies": {
    "@atomist/automation-client": "1.0.0-master.20181010204819",
    "@atomist/automation-client-ext-logzio": "1.0.0-master.20181010130620",
    "@atomist/automation-client-ext-raven": "1.0.0-master.20181010130149",
    "@atomist/clj-editors": "0.5.7",
    "@atomist/sdm": "1.0.0-master.20181010145707",
    "@atomist/sdm-core": "1.0.0-master.20181011053254",
    "@atomist/sdm-pack-clojure": "1.0.0-master.20181011140109",
    "@atomist/sdm-pack-docker": "1.0.0-master.20181011112818",
    "@atomist/sdm-pack-fingerprints": "^1.1.1",
    "@atomist/sdm-pack-node": "1.0.0-master.20181011124853",
    "@atomist/sdm-pack-rcca": "0.0.5-master.20181011150101",
    "@atomist/slack-messages": "1.0.1-master.20180830190736",
    "dateformat": "^3.0.3",
    "fs-extra": "^6.0.0",
    "lodash": "^4.17.10",
    "node-dir": "^0.1.17",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^5.0.4",
    "@types/lodash": "^4.14.116",
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.6.28",
    "@types/node-dir": "0.0.32",
    "@types/power-assert": "^1.4.29",
    "@types/uuid": "^3.4.3",
    "apollo-codegen": "^0.19.1",
    "espower-typescript": "^8.1.3",
    "graphql-code-generator": "^0.8.19",
    "mocha": "^4.1.0",
    "npm-run-all": "^4.1.2",
    "nyc": "^13.0.1",
    "power-assert": "^1.4.4",
    "prettier": "^1.14.2",
    "rimraf": "^2.6.2",
    "source-map-support": "^0.5.9",
    "supervisor": "^0.12.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.11.0",
    "typedoc": "^0.11.1",
    "typescript": "2.9.2"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "autostart": "nodemon --watch index.ts --watch lib --ext ts --delay 4 --exec ts-node -- node_modules/@atomist/automation-client/bin/start.js",
    "autotest": "supervisor --watch index.ts,lib,test --extensions ts --no-restart-on exit --quiet --exec npm -- test",
    "build": "run-s compile test lint doc",
    "clean": "run-p clean:compile clean:test clean:doc clean:run",
    "clean:compile": "rimraf git-info.json \"index.{d.ts,js{,.map}}\" \"{lib,test}/**/*.{d.ts,js{,.map}}\" lib/typings/types.ts",
    "clean:dist": "run-s clean clean:npm",
    "clean:doc": "rimraf doc",
    "clean:npm": "rimraf node_modules",
    "clean:run": "rimraf *-v8.log profile.txt log",
    "clean:test": "rimraf .nyc_output coverage",
    "compile": "run-s git:info gql:gen compile:ts",
    "compile:ts": "tsc --project .",
    "doc": "typedoc --mode modules --excludeExternals --ignoreCompilerErrors --exclude \"**/*.d.ts\" --out doc index.ts lib",
    "git:info": "atm-git-info",
    "gql:gen": "atm-gql-gen",
    "lint": "tslint --format verbose --project . --exclude \"node_modules/**\" --exclude \"**/*.d.ts\" \"**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "start": "atm-start",
    "test": "nyc mocha --require espower-typescript/guess --require source-map-support/register \"test/**/*.test.ts\"",
    "test:one": "mocha --require espower-typescript/guess \"test/**/${TEST:-*.test.ts}\"",
    "typedoc": "npm run doc"
  },
  "engines": {
    "node": "9.x.x",
    "npm": "5.x.x"
  },
  "nyc": {
    "include": [
      "lib/**/*.*"
    ],
    "extension": [
      ".ts"
    ],
    "exclude": [
      "lib/typings",
      "**/*.d.ts"
    ],
    "reporter": [
      "html",
      "text-summary"
    ],
    "all": true
  }
}
