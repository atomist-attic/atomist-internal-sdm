{
  "name": "@atomist/atomist-internal-sdm",
  "version": "0.3.65",
  "description": "an Atomist SDM to deliver Clojure projects",
  "author": "Atomist, Inc.",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/atomist/atomist-internal-sdm.git"
  },
  "keywords": [
    "atomist",
    "automation",
    "sdm",
    "clojure",
    "lein"
  ],
  "homepage": "https://github.com/atomist/atomist-internal-sdm#readme",
  "bugs": {
    "url": "https://github.com/atomist/atomist-internal-sdm/issues"
  },
  "dependencies": {
    "@atomist/automation-client": "1.4.1-master.20190430085123",
    "@atomist/automation-client-ext-dashboard": "1.0.2-master.20190107121940",
    "@atomist/automation-client-ext-humio": "^1.0.3-master.20190319172045",
    "@atomist/automation-client-ext-logzio": "1.0.2-master.20181213233859",
    "@atomist/automation-client-ext-raven": "1.0.2-master.20181213200324",
    "@atomist/clj-editors": "0.7.1",
    "@atomist/sdm": "1.5.0-master.20190429215139",
    "@atomist/sdm-core": "1.5.0-master.20190429220018",
    "@atomist/sdm-pack-build": "1.0.4-master.20181217220011",
    "@atomist/sdm-pack-clojure": "1.0.16",
    "@atomist/sdm-pack-docker": "^1.2.0",
    "@atomist/sdm-pack-fingerprints": "3.1.0",
    "@atomist/sdm-pack-issue": "1.1.0",
    "@atomist/sdm-pack-node": "1.0.3-master.20190110120831",
    "@atomist/slack-messages": "^1.1.0",
    "@kubernetes/client-node": "^0.8.2",
    "dateformat": "^3.0.3",
    "fs-extra": "^6.0.0",
    "lodash": "^4.17.10",
    "node-dir": "^0.1.17",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^5.0.4",
    "@types/lodash": "^4.14.116",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.0.7",
    "@types/power-assert": "^1.5.0",
    "@types/uuid": "^3.4.3",
    "espower-typescript": "^9.0.2",
    "mocha": "^6.1.4",
    "npm-run-all": "^4.1.5",
    "power-assert": "^1.6.1",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "source-map-support": "^0.5.9",
    "supervisor": "^0.12.0",
    "ts-node": "8.2.0",
    "tslint": "^5.17.0",
    "typedoc": "^0.14.2",
    "typescript": "3.5.1"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "autotest": "supervisor --watch index.ts,lib,test --extensions ts --no-restart-on exit --quiet --exec npm -- test",
    "build": "npm-run-all gql:gen compile:ts test lint doc",
    "clean": "npm-run-all clean:compile clean:doc clean:run",
    "clean:compile": "rimraf git-info.json \"index.{d.ts,js{,.map}}\" \"{lib,test}/**/*.{d.ts,js{,.map}}\" lib/typings/types.ts",
    "clean:dist": "run-s clean clean:npm",
    "clean:doc": "rimraf doc",
    "clean:npm": "rimraf node_modules",
    "clean:run": "rimraf *-v8.log profile.txt log",
    "compile": "npm-run-all gql:gen compile:ts",
    "compile:ts": "tsc --project .",
    "doc": "typedoc --mode modules --ignoreCompilerErrors --exclude \"**/*.d.ts\" --out doc index.ts lib",
    "git:info": "atm-git-info",
    "gql:gen": "atm-gql-gen",
    "lint": "tslint --format verbose --project . --exclude \"{build,node_modules}/**\" --exclude \"**/*.d.ts\" \"**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "start": "atm-start",
    "test": "mocha --require espower-typescript/guess \"test/**/*.test.ts\"",
    "test:one": "mocha --require espower-typescript/guess \"test/**/${TEST:-*.test.ts}\"",
    "typedoc": "npm run doc"
  },
  "engines": {
    "node": ">=8.1.0",
    "npm": ">=6.0.0"
  }
}
